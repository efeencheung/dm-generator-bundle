{{ "{% extends 'DmThemeBundle::layout.html.twig' %}" }}

{{ "{% block breabcrumb %}" }}
<div class="page-bar">
    <ul class="page-breadcrumb">
        <li>
            <i class="fa fa-dashboard"></i> <a href="{{ "{{ path('dashboard')}}" }}">控制台</a>
        </li>
        <li><i class="fa fa-angle-right"></i></li>
        <li>
            <a href="{{ "{{ path('" ~ route_name_prefix ~ "')}}" }}">{{ entity_chinese_name }}管理</a>
        </li>
        <li><i class="fa fa-angle-right"></i></li>
        <li>
            <span>{{ entity_chinese_name }}创建</span>
        </li>
    </ul>
</div>
{{ "{% endblock %}" }}

{{ "{% block content_wrapper -%}" }}
<div class="row">
    <div class="col-md-12">
        <div class="portlet light bordered">
            <div class="portlet-title">
                <div class="caption">
                    <i class=" icon-plus font-green"></i>
                    <span class="caption-subject font-green sbold uppercase">{{ entity_chinese_name }}创建</span>
                </div>
                <div class="actions">
                    <a href="{{ "{{ path('" ~ route_name_prefix ~ "')}}" }}" class="btn btn-sm green">返回{{ entity_chinese_name }}列表</a>
                </div>
            </div>
            <div class="portlet-body form">
                {{ "{{ form_start(form) }}" }}
                    {{ "{{ form_widget(form)}}" }}
                    <div class="form-actions">
                        <div class="row">
                            <div class="col-md-10 col-md-offset-2">
                                <input class="btn green" type="submit" value="提交" />
                            </div>
                        </div>
                    </div>
                {{ "{{ form_end(form) }}" }}
            </div>
        </div>
    </div>
</div>
{{ "{% endblock %}" }}
